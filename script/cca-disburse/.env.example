# Chain ID (421614 = Arbitrum Sepolia, 42161 = Arbitrum One)
CHAIN_ID=421614

# RPC endpoint for the target chain
RPC_URL=https://sepolia-rollup.arbitrum.io/rpc

# Private key of the disburser account (with or without 0x prefix)
DISBURSER_PRIVATE_KEY=

# Address of the CCADisbursementTracker contract
TRACKER_TOKEN_ADDRESS=

# Address of the CCA (ContinuousClearingAuction) contract
CCA_ADDRESS=

# Address of the original token being disbursed (e.g., IDOS token)
SOLD_TOKEN_ADDRESS=

# Address of the WhaleDisburser contract
WHALE_DISBURSER_ADDRESS=

# Timestamp when the Normal phase starts (ISO 8601, Zulu time). Bids in blocks at or after
# this timestamp are Normal phase; earlier blocks are Whale phase.
NORMAL_PHASE_START=

# Unix timestamp for when whale vesting begins. Duration (150 days) and cliff
# (30 days) are hardcoded in the WhaleDisburser contract.
VESTING_START=

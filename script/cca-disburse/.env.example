# RPC endpoint for Arbitrum Sepolia
RPC_URL=https://sepolia-rollup.arbitrum.io/rpc

# Private key of the disburser account (with or without 0x prefix)
DISBUSER_DISBURSER_PRIVATE_KEY=

# Address of the CCADisbursementTracker contract
TRACKER_ADDRESS=

# Address of the CCA (ContinuousClearingAuction) contract
CCA_ADDRESS=

# Address of the original token being disbursed (e.g., IDOS token)
SOLD_TOKEN_ADDRESS=

# Address of the Disperse contract (for batched token transfers)
DISPERSE_ADDRESS=

# Address of the TokenOps vesting contract (receives whale vested tokens)
TOKENOPS_ADDRESS=

# Timestamp when the Normal phase starts (ISO 8601). Bids in blocks at or after
# this timestamp are Normal phase; earlier blocks are Whale phase.
NORMAL_PHASE_START=2025-03-15T12:00:00Z

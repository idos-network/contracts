{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ES2022",
		"moduleResolution": "bundler",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"outDir": "dist",
		"rootDir": "src"
	},
	"include": ["src"]
}
